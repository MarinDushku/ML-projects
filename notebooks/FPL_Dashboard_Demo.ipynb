    "# Cell 2: Load Trained Models and Data\n",
    "\n",
    "print(\"ü§ñ Loading trained FPL models...\")\n",
    "\n",
    "# Load the most recent feature data\n",
    "feature_files = glob.glob('data/processed/*player_features*.csv')\n",
    "if feature_files:\n",
    "    latest_features = max(feature_files, key=os.path.getctime)\n",
    "    features_df = pd.read_csv(latest_features)\n",
    "    print(f\"‚úÖ Loaded features: {latest_features}\")\n",
    "    print(f\"üìä Features shape: {features_df.shape}\")\nelse:\n",
    "    print(\"‚ùå No feature files found. Please run FPL_Feature_Engineering.ipynb first.\")\n",
    "    features_df = pd.DataFrame()\n",
    "\n",
    "# Load player names and basic info\n",
    "player_files = glob.glob('data/raw/*players*.csv')\n",
    "if player_files:\n",
    "    latest_players = max(player_files, key=os.path.getctime)\n",
    "    players_df = pd.read_csv(latest_players)\n",
    "    print(f\"‚úÖ Loaded players: {latest_players}\")\nelse:\n",
    "    print(\"‚ùå No player files found.\")\n",
    "    players_df = pd.DataFrame()\n",
    "\n",
    "# Create ensemble predictor class for demo\n",
    "class FPLDashboardPredictor:\n",
    "    \"\"\"Simplified predictor for dashboard demo.\"\"\"\n",
    "    \n",
    "    def __init__(self, features_df, players_df):\n",
    "        self.features_df = features_df\n",
    "        self.players_df = players_df\n",
    "        self.position_weights = {\n",
    "            'Goalkeeper': {'clean_sheets': 4, 'saves': 0.33, 'bonus': 1},\n",
    "            'Defender': {'clean_sheets': 4, 'goals_scored': 6, 'assists': 3, 'bonus': 1},\n",
    "            'Midfielder': {'goals_scored': 5, 'assists': 3, 'clean_sheets': 1, 'bonus': 1},\n",
    "            'Forward': {'goals_scored': 4, 'assists': 3, 'bonus': 1}\n",
    "        }\n",
    "    \n",
    "    def predict_points(self, gameweek=None):\n",
    "        \"\"\"Generate predictions for all players.\"\"\"\n",
    "        predictions = []\n",
    "        \n",
    "        for _, player in self.features_df.iterrows():\n",
    "            # Simple prediction based on form and fixtures\n",
    "            base_points = player.get('form_points_5gw', 2.0)\n",
    "            fixture_adjustment = (6 - player.get('fixture_difficulty', 3)) * 0.5\n",
    "            home_bonus = 0.5 if player.get('is_home', 0.5) > 0.5 else 0\n",
    "            \n",
    "            predicted_points = max(0, base_points + fixture_adjustment + home_bonus)\n",
    "            \n",
    "            # Add some position-specific logic\n",
    "            position = player.get('position', 'Midfielder')\n",
    "            if position == 'Goalkeeper':\n",
    "                clean_sheet_prob = player.get('clean_sheet_probability', 0.3)\n",
    "                predicted_points += clean_sheet_prob * 4\n",
    "            elif position == 'Forward':\n",
    "                goal_rate = player.get('goal_scoring_rate', 0.2)\n",
    "                predicted_points += goal_rate * 4\n",
    "            \n",
    "            predictions.append({\n",
    "                'player_id': player['player_id'],\n",
    "                'position': position,\n",
    "                'predicted_points': predicted_points,\n",
    "                'confidence': min(1.0, predicted_points / 10),\n",
    "                'fixture_difficulty': player.get('fixture_difficulty', 3),\n",
    "                'form_5gw': player.get('form_points_5gw', 2),\n",
    "                'price': player.get('price', 5.0),\n",
    "                'ownership': player.get('ownership_percentage', 10)\n",
    "            })\n",
    "        \n",
    "        pred_df = pd.DataFrame(predictions)\n",
    "        \n",
    "        # Merge with player names\n",
    "        if not self.players_df.empty:\n",
    "            name_mapping = self.players_df.set_index('id')[['first_name', 'second_name', 'web_name', 'team_name']].to_dict('index')\n",
    "            \n",
    "            pred_df['player_name'] = pred_df['player_id'].map(\n",
    "                lambda x: name_mapping.get(x, {}).get('web_name', f'Player {x}')\n",
    "            )\n",
    "            pred_df['full_name'] = pred_df['player_id'].map(\n",
    "                lambda x: f\"{name_mapping.get(x, {}).get('first_name', '')} {name_mapping.get(x, {}).get('second_name', '')}\".strip() or f'Player {x}'\n",
    "            )\n",
    "            pred_df['team'] = pred_df['player_id'].map(\n",
    "                lambda x: name_mapping.get(x, {}).get('team_name', 'Unknown')\n",
    "            )\n",
    "        else:\n",
    "            pred_df['player_name'] = pred_df['player_id'].apply(lambda x: f'Player {x}')\n",
    "            pred_df['full_name'] = pred_df['player_name']\n",
    "            pred_df['team'] = 'Unknown'\n",
    "        \n",
    "        return pred_df.sort_values('predicted_points', ascending=False)\n",
    "\n",
    "# Initialize predictor\n",
    "if not features_df.empty:\n",
    "    predictor = FPLDashboardPredictor(features_df, players_df)\n",
    "    print(\"‚úÖ Dashboard predictor initialized!\")\n",
    "    \n",
    "    # Generate sample predictions\n",
    "    sample_predictions = predictor.predict_points()\n",
    "    print(f\"üìä Generated {len(sample_predictions)} predictions\")\n",
    "    \n",
    "    # Show top predictions by position\n",
    "    print(\"\\nüèÜ Top Predicted Players by Position:\")\n",
    "    for position in ['Goalkeeper', 'Defender', 'Midfielder', 'Forward']:\n",
    "        pos_players = sample_predictions[sample_predictions['position'] == position]\n",
    "        if not pos_players.empty:\n",
    "            top_player = pos_players.iloc[0]\n",
    "            print(f\"  {position}: {top_player['player_name']} - {top_player['predicted_points']:.1f} pts\")\nelse:\n",
    "    print(\"‚ùå Cannot initialize predictor without feature data\")\n",
    "    predictor = None\n",
    "    sample_predictions = pd.DataFrame()"
   ]
