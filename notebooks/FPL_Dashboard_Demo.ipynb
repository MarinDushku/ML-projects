    "# Cell 4: Team Optimization Dashboard\n",
    "\n",
    "print(\"üèÜ Creating Team Optimization Dashboard...\")\n",
    "\n",
    "if sample_predictions.empty:\n",
    "    print(\"‚ùå No predictions available for team optimization\")\nelse:\n",
    "    class FPLTeamOptimizer:\n",
    "        \"\"\"Simple team optimization for FPL.\"\"\"\n",
    "        \n",
    "        def __init__(self, predictions_df):\n",
    "            self.predictions = predictions_df\n",
    "            self.formation_constraints = {\n",
    "                'Goalkeeper': (2, 2),  # min, max\n",
    "                'Defender': (5, 5),\n",
    "                'Midfielder': (5, 5),\n",
    "                'Forward': (3, 3)\n",
    "            }\n",
    "        \n",
    "        def optimize_team(self, budget=100.0, exclude_players=None):\n",
    "            \"\"\"Simple greedy optimization for team selection.\"\"\"\n",
    "            if exclude_players is None:\n",
    "                exclude_players = []\n",
    "            \n",
    "            # Filter out excluded players\n",
    "            available_players = self.predictions[\n",
    "                ~self.predictions['player_id'].isin(exclude_players)\n",
    "            ].copy()\n",
    "            \n",
    "            # Calculate value score (points per price)\n",
    "            available_players['value_score'] = (\n",
    "                available_players['predicted_points'] / available_players['price']\n",
    "            )\n",
    "            \n",
    "            selected_team = []\n",
    "            remaining_budget = budget\n",
    "            position_counts = {pos: 0 for pos in self.formation_constraints.keys()}\n",
    "            \n",
    "            # Sort by value score\n",
    "            available_players = available_players.sort_values('value_score', ascending=False)\n",
    "            \n",
    "            for _, player in available_players.iterrows():\n",
    "                position = player['position']\n",
    "                price = player['price']\n",
    "                \n",
    "                # Check constraints\n",
    "                min_pos, max_pos = self.formation_constraints[position]\n",
    "                \n",
    "                if (position_counts[position] < max_pos and \n",
    "                    price <= remaining_budget and\n",
    "                    len(selected_team) < 15):\n",
    "                    \n",
    "                    selected_team.append(player)\n",
    "                    position_counts[position] += 1\n",
    "                    remaining_budget -= price\n",
    "                    \n",
    "                    # Check if we have a complete team\n",
    "                    if len(selected_team) == 15:\n",
    "                        break\n",
    "            \n",
    "            team_df = pd.DataFrame(selected_team)\n",
    "            \n",
    "            return {\n",
    "                'team': team_df,\n",
    "                'total_cost': budget - remaining_budget,\n",
    "                'remaining_budget': remaining_budget,\n",
    "                'total_predicted_points': team_df['predicted_points'].sum(),\n",
    "                'position_counts': position_counts\n",
    "            }\n",
    "        \n",
    "        def get_best_xi(self, team_df):\n",
    "            \"\"\"Select best XI from 15-man squad.\"\"\"\n",
    "            formation = {'Goalkeeper': 1, 'Defender': 4, 'Midfielder': 4, 'Forward': 2}\n",
    "            \n",
    "            best_xi = []\n",
    "            for position, count in formation.items():\n",
    "                position_players = team_df[team_df['position'] == position]\n",
    "                if len(position_players) >= count:\n",
    "                    best = position_players.nlargest(count, 'predicted_points')\n",
    "                    best_xi.append(best)\n",
    "            \n",
    "            return pd.concat(best_xi, ignore_index=True) if best_xi else pd.DataFrame()\n",
    "    \n",
    "    # Initialize optimizer\n",
    "    optimizer = FPLTeamOptimizer(sample_predictions)\n",
    "    \n",
    "    # Create optimization widgets\n",
    "    budget_slider = widgets.FloatSlider(\n",
    "        value=100.0,\n",
    "        min=90.0,\n",
    "        max=110.0,\n",
    "        step=0.5,\n",
    "        description='Budget (¬£):',\n",
    "        style={'description_width': 'initial'}\n",
    "    )\n",
    "    \n",
    "    optimize_button = widgets.Button(\n",
    "        description='üèÜ Optimize Team',\n",
    "        button_style='success',\n",
    "        layout=widgets.Layout(width='200px')\n",
    "    )\n",
    "    \n",
    "    team_output = widgets.Output()\n",
    "    \n",
    "    def optimize_team_callback(button):\n",
    "        with team_output:\n",
    "            clear_output(wait=True)\n",
    "            \n",
    "            print(f\"üîÑ Optimizing team with ¬£{budget_slider.value}M budget...\")\n",
    "            \n",
    "            # Optimize team\n",
    "            result = optimizer.optimize_team(budget=budget_slider.value)\n",
    "            team_df = result['team']\n",
    "            \n",
    "            if team_df.empty:\n",
    "                print(\"‚ùå Could not create a valid team with the given constraints\")\n",
    "                return\n",
    "            \n",
    "            # Get best XI\n",
    "            best_xi = optimizer.get_best_xi(team_df)\n",
    "            \n",
    "            # Create visualization\n",
    "            fig = make_subplots(\n",
    "                rows=2, cols=2,\n",
    "                subplot_titles=[\n",
    "                    'Squad by Position',\n",
    "                    'Best XI Formation',\n",
    "                    'Points Distribution',\n",
    "                    'Budget Breakdown'\n",
    "                ],\n",
    "                specs=[[{\"type\": \"bar\"}, {\"type\": \"bar\"}],\n",
