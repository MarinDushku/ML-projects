    "                row=1, col=2\n",
    "            )\n",
    "            \n",
    "            # Risk distribution\n",
    "            risk_counts = candidates['risk_category'].value_counts()\n",
    "            fig.add_trace(\n",
    "                go.Bar(\n",
    "                    x=risk_counts.index,\n",
    "                    y=risk_counts.values,\n",
