    "# Cell 5: Captain Selection Intelligence\n",
    "\n",
    "print(\"üëë Creating Captain Selection Dashboard...\")\n",
    "\n",
    "if sample_predictions.empty:\n",
    "    print(\"‚ùå No predictions available for captain analysis\")\nelse:\n",
    "    class CaptainSelector:\n",
    "        \"\"\"Intelligent captain selection with risk analysis.\"\"\"\n",
    "        \n",
    "        def __init__(self, predictions_df):\n",
    "            self.predictions = predictions_df\n",
    "        \n",
    "        def analyze_captains(self, top_n=15, min_ownership=5.0):\n",
    "            \"\"\"Analyze potential captains.\"\"\"\n",
    "            # Filter captain candidates\n",
    "            candidates = self.predictions[\n",
    "                (self.predictions['predicted_points'] >= 6) &  # Minimum expected points\n",
    "                (self.predictions['ownership'] >= min_ownership)  # Minimum ownership\n",
    "            ].copy()\n",
    "            \n",
    "            # Calculate captain metrics\n",
    "            candidates['captain_score'] = (\n",
    "                candidates['predicted_points'] * 2 +  # Doubled points\n",
    "                candidates['confidence'] * 3 +  # Confidence boost\n",
    "                (6 - candidates['fixture_difficulty']) * 0.5  # Fixture adjustment\n",
    "            )\n",
    "            \n",
    "            # Risk assessment\n",
    "            candidates['risk_level'] = (\n",
    "                candidates['fixture_difficulty'] * 0.3 +\n",
    "                (100 - candidates['ownership']) * 0.01 +  # Lower ownership = higher risk\n",
    "                (10 - candidates['form_5gw']) * 0.1  # Poor form = higher risk\n",
    "            )\n",
    "            \n",
    "            # Risk categories\n",
    "            candidates['risk_category'] = pd.cut(\n",
    "                candidates['risk_level'], \n",
    "                bins=[0, 2, 4, 6, 10], \n",
    "                labels=['Low Risk', 'Medium Risk', 'High Risk', 'Very High Risk']\n",
    "            )\n",
    "            \n",
    "            # Expected captain points (doubled)\n",
    "            candidates['expected_captain_points'] = candidates['predicted_points'] * 2\n",
    "            \n",
    "            # Differential potential\n",
    "            candidates['differential_potential'] = (\n",
    "                candidates['predicted_points'] * (100 - candidates['ownership']) / 100\n",
    "            )\n",
    "            \n",
    "            return candidates.nlargest(top_n, 'captain_score')\n",
    "        \n",
    "        def get_captain_recommendations(self, risk_tolerance='medium'):\n",
    "            \"\"\"Get captain recommendations based on risk tolerance.\"\"\"\n",
    "            candidates = self.analyze_captains()\n",
    "            \n",
    "            risk_filters = {\n",
    "                'low': ['Low Risk'],\n",
    "                'medium': ['Low Risk', 'Medium Risk'],\n",
    "                'high': ['Low Risk', 'Medium Risk', 'High Risk'],\n",
    "                'very_high': ['Low Risk', 'Medium Risk', 'High Risk', 'Very High Risk']\n",
    "            }\n",
    "            \n",
    "            filtered = candidates[\n",
    "                candidates['risk_category'].isin(risk_filters.get(risk_tolerance, ['Low Risk', 'Medium Risk']))\n",
    "            ]\n",
    "            \n",
    "            return {\n",
    "                'safe_captain': filtered.nlargest(1, 'expected_captain_points'),\n",
    "                'differential_captain': filtered.nlargest(1, 'differential_potential'),\n",
    "                'all_candidates': filtered\n",
    "            }\n",
    "    \n",
    "    # Initialize captain selector\n",
    "    captain_selector = CaptainSelector(sample_predictions)\n",
    "    \n",
    "    # Create captain analysis widgets\n",
    "    risk_tolerance = widgets.Dropdown(\n",
    "        options=[('Conservative', 'low'), ('Balanced', 'medium'), ('Aggressive', 'high'), ('Punt', 'very_high')],\n",
    "        value='medium',\n",
    "        description='Risk Tolerance:',\n",
    "        style={'description_width': 'initial'}\n",
    "    )\n",
    "    \n",
    "    min_ownership_slider = widgets.FloatSlider(\n",
    "        value=5.0,\n",
    "        min=0.0,\n",
    "        max=50.0,\n",
    "        step=1.0,\n",
    "        description='Min Ownership %:',\n",
    "        style={'description_width': 'initial'}\n",
    "    )\n",
    "    \n",
    "    analyze_button = widgets.Button(\n",
    "        description='üëë Analyze Captains',\n",
    "        button_style='info',\n",
    "        layout=widgets.Layout(width='200px')\n",
    "    )\n",
    "    \n",
    "    captain_output = widgets.Output()\n",
    "    \n",
    "    def analyze_captains_callback(button):\n",
    "        with captain_output:\n",
    "            clear_output(wait=True)\n",
    "            \n",
    "            print(f\"üëë Analyzing captain options...\")\n",
    "            \n",
    "            # Get captain analysis\n",
    "            candidates = captain_selector.analyze_captains(min_ownership=min_ownership_slider.value)\n",
    "            recommendations = captain_selector.get_captain_recommendations(risk_tolerance.value)\n",
    "            \n",
    "            if candidates.empty:\n",
    "                print(\"‚ùå No suitable captain candidates found\")\n",
    "                return\n",
    "            \n",
    "            # Create captain analysis visualization\n",
    "            fig = make_subplots(\n",
    "                rows=2, cols=2,\n",
    "                subplot_titles=[\n",
    "                    'Captain Score vs Risk Level',\n",
    "                    'Expected Points vs Ownership',\n",
