    "                    textposition='outside',\n",
    "                    marker_color=['green', 'yellow', 'orange', 'red'][:len(risk_counts)],\n",
    "                    showlegend=False\n",
    "                ),\n",
    "                row=2, col=1\n",
    "            )\n",
    "            \n",
    "            # Differential potential\n",
    "            top_diffs = candidates.nlargest(8, 'differential_potential')\n",
    "            fig.add_trace(\n",
    "                go.Bar(\n",
    "                    x=top_diffs['player_name'],\n",
    "                    y=top_diffs['differential_potential'],\n",
    "                    text=top_diffs['differential_potential'].round(1),\n",
    "                    textposition='outside',\n",
    "                    marker_color=top_diffs['ownership'],\n",
    "                    marker_colorscale='Viridis_r',\n",
    "                    showlegend=False\n",
    "                ),\n",
    "                row=2, col=2\n",
    "            )\n",
    "            \n",
    "            fig.update_layout(\n",
    "                height=800,\n",
    "                title_text=\"Captain Selection Analysis\",\n",
    "                showlegend=False\n",
    "            )\n",
    "            \n",
    "            # Update axis labels\n",
    "            fig.update_xaxes(title_text=\"Risk Level\", row=1, col=1)\n",
    "            fig.update_yaxes(title_text=\"Captain Score\", row=1, col=1)\n",
    "            fig.update_xaxes(title_text=\"Ownership %\", row=1, col=2)\n",
    "            fig.update_yaxes(title_text=\"Expected Captain Points\", row=1, col=2)\n",
    "            fig.update_xaxes(title_text=\"Risk Category\", row=2, col=1)\n",
    "            fig.update_yaxes(title_text=\"Number of Players\", row=2, col=1)\n",
    "            fig.update_xaxes(title_text=\"Player\", row=2, col=2)\n",
    "            fig.update_yaxes(title_text=\"Differential Potential\", row=2, col=2)\n",
    "            \n",
    "            fig.show()\n",
    "            \n",
    "            # Show recommendations\n",
    "            print(f\"\\nüëë Captain Recommendations ({risk_tolerance.options[risk_tolerance.index][0]} Strategy):\")\n",
    "            \n",
    "            safe_captain = recommendations['safe_captain']\n",
    "            if not safe_captain.empty:\n",
    "                safe = safe_captain.iloc[0]\n",
    "                print(f\"\\nüõ°Ô∏è  SAFE CAPTAIN: {safe['player_name']}\")\n",
    "                print(f\"   Expected Points: {safe['expected_captain_points']:.1f}\")\n",
    "                print(f\"   Risk Level: {safe['risk_category']}\")\n",
    "                print(f\"   Ownership: {safe['ownership']:.1f}%\")\n",
    "            \n",
    "            diff_captain = recommendations['differential_captain']\n",
