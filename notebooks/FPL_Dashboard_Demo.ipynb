    "                ],\n",
    "                specs=[[{\"type\": \"scatter\"}, {\"type\": \"scatter\"}],\n",
    "                       [{\"type\": \"bar\"}, {\"type\": \"bar\"}]]\n",
    "            )\n",
    "            \n",
    "            # Captain Score vs Risk\n",
    "            fig.add_trace(\n",
    "                go.Scatter(\n",
    "                    x=candidates['risk_level'],\n",
